<div id="allListBody">
  <nav class=" d-flex p-2" id="allListNavBar">
    <a type="button" id="allListNavBTN" class=" btn btn-outline-light" href="/" id="HomeBTN">Home</a>
    {{#if loggedIn}}
    <button type="button" id="allListNavBTN" class="btn btn-outline-light"><a href="/" id="logoutNavBTN">Log
        Out</a></button>
    {{/if}}
    <a type="button" id="allListNavBTN" class="btn btn-outline-light" href="/list/:id">My list</a>
    <a type="button" id="allListNavBTN" class="btn btn-outline-light" href="/aboutus">About us</a>
    <a type="button" id="allListNavBTN" class=" btn btn-outline-light" href="/toppicks">Top
      Picks</a>
  </nav>
  <h1 id="titleHeaderTextAllList" class="d-flex">Rankedify</h1>
  <div>
    <div>
      <h2 id="allListDescription">Check out everyone's top 5 list!</h2>
    </div>
    <div>
      <div id="scrollList">
        {{#each list}}
        <ul id="allListUlBody">
          <p>{{this.username}}'s Top 5 List!</p>
          <li>1. {{this.user_input.game_input_one}}</li>
          <li>2. {{this.user_input.game_input_two}}</li>
          <li>3. {{this.user_input.game_input_three}}</li>
          <li>4. {{this.user_input.game_input_four}}</li>
          <li>5. {{this.user_input.game_input_five}}</li>
        </ul>
        {{/each}}
      </div>
    </div>
  </div>
  {{#if loggedIn}}
  <script src="/js/mylist.js"></script>
  <script src="/js/logout.js"></script>
  {{/if}}
  <script src="/js/login.js"></script>
</div>