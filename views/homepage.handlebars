        <button type="button" id="navBTN-Homepage" class="btn btn-outline-light navBTN-Homepage"><a href="/"
                id="logoutNavBTN">Log
                Out</a></button>
        {{else}}
        <button type="button" id="navBTN-Homepage" class=" btn btn-outline-light navBTN-Homepage"><a href="/login"
                id="loginInNavBTN">Log
                in</a></button>
        {{/if}}
        <button type="button" id="myList" class="btn btn-outline-light raise navBTN-Homepage"><a href="/list/:id">My
